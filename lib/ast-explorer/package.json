{
  "name": "meld-ast-explorer",
  "version": "0.1.0",
  "description": "AST explorer tools for Meld grammar",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ast-explorer": "./bin/ast-explorer.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "cli": "ts-node src/command.ts",
    "explore": "ts-node src/command.ts explore",
    "extract": "ts-node src/command.ts extract",
    "snapshot": "ts-node src/command.ts snapshot",
    "compare": "ts-node src/command.ts compare",
    "batch": "ts-node src/command.ts batch",
    "generate-types": "ts-node scripts/generate-types.ts",
    "generate-fixture": "ts-node src/command.ts generate-fixture",
    "workflow": "ts-node src/command.ts workflow",
    "process-all": "ts-node src/command.ts process-all",
    "validate": "ts-node src/command.ts validate",
    "example": "ts-node examples/usage.ts",
    "init": "ts-node src/command.ts init"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "memfs": "^4.17.1",
    "fs-extra": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "vitest": "^0.30.0"
  }
}
