{
  "security": {
    "urls": {
      "enabled": true,
      "allowedDomains": [
        "github.com",
        "raw.githubusercontent.com",
        "gist.githubusercontent.com",
        "api.github.com"
      ],
      "blockedDomains": [
        "example-malware.com"
      ],
      "allowedProtocols": ["https"],
      "maxSize": "10MB",
      "timeout": "30s",
      "warnOnInsecureProtocol": true
    }
  },
  "cache": {
    "urls": {
      "enabled": true,
      "defaultTTL": "5m",
      "rules": [
        {
          "pattern": "https://api.github.com/*",
          "ttl": "1m"
        },
        {
          "pattern": "https://raw.githubusercontent.com/*",
          "ttl": "1h"
        },
        {
          "pattern": "*.md",
          "ttl": "24h"
        },
        {
          "pattern": "*/releases/*",
          "ttl": "10m"
        }
      ]
    }
  }
}