/exe @processRequest(method, path, auth) = when: [
  @method == "GET" && @path == "/users" => @listUsers()
  @method == "POST" && @path == "/users" => @createUser(@auth)
  @method == "DELETE" || @method == "PUT" => "Method not allowed"
  !@auth => "Unauthorized"
  true => @defaultHandler()
]

