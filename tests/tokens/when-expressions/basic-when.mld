/exe @calculate(op, a, b) = when [
  @op == "add" => @add(@a, @b)
  @op == "multiply" => @multiply(@a, @b)
  @op == "divide" && @b != 0 => @divide(@a, @b)
  @op == "divide" => "Error: Division by zero"
  true => "Unknown operation"
]

