/var @jsResult = js { return "JavaScript"; }
/var @pyResult = python { return "Python" }
/var @bashResult = bash { echo "Bash" }

/exe @polyglot(lang) = when: [
  @lang == "js" => js { return "Running JavaScript"; }
  @lang == "python" => python { return "Running Python" }
  @lang == "bash" => bash { echo "Running Bash" }
  true => "Unknown language"
]

