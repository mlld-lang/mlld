/when first: [
  @method == "GET" && @path == "/api/users" => @listUsers()
  @method == "POST" && @path == "/api/users" => @createUser()
  @method == "PUT" && @path == "/api/users/*" => @updateUser(@params.id)
  @method == "DELETE" => /show `Method not allowed`
  true => /show `Unknown request`
]

