/when [
  @x == "implicit assignment" => @process()
  @y == "hello" => var @y = "explicit assignment"
  @z == "bye" => @applyFilter(@data)
]

=== START TOKENS ===
[ --> operator
@x --> variable
== --> operator
"implicit assignment" --> string
=> --> operator
@process --> variable
( --> operator
) --> operator
@y --> variable
== --> operator
"hello" --> string
=> --> operator
var --> keyword
@y --> variable[declaration]
= --> operator
"explicit assignment" --> string
@z --> variable
== --> operator
"bye" --> string
=> --> operator
@applyFilter --> variable
( --> operator
@data --> variable
) --> operator
] --> operator
=== END TOKENS ===
