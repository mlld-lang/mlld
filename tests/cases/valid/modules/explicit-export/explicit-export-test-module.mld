# Internal helper - not exported
/var @_internalHelper = "This is internal"

# Public functions
@text greet(name) = [[Hello, {{name}}!]]
@text farewell(name) = [[Goodbye, {{name}}!]]

# Explicit module export - only greet and farewell are exported
/var @module = {
greet: @greet,
farewell: @farewell
}