var tools @agentTools = {
  searchIssues: {
    mlld: @searchIssues,
    bind: { org: "mlld-lang", repo: "mlld" },
    expose: ["query"],
    labels: ["read-only"],
    description: "Search mlld issues"
  },
  createIssue: {
    mlld: @createIssue,
    bind: { org: "mlld-lang", repo: "mlld" },
    expose: ["title", "body"],
    labels: ["destructive"],
    description: "Create an mlld issue"
  }
}